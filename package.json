{
  "name": "starter-kit-theme",
  "description": "WordPress starter theme with a modern development stack for launching projects faster and easily",
  "engines": {
    "node": ">=18.0.0 <19",
    "npm": ">=9 <10"
  },
  "dependencies": {
    "@wordpress/babel-preset-default": "7.28.0",
    "bootstrap": "5.3.2",
    "font-awesome": "4.7.0",
    "glob": "10.0.*",
    "laravel-mix": "6.0.49",
    "sass": "1.69.4",
    "sass-loader": "13.3.2"
  },
  "devDependencies": {
    "browser-sync": "2.29.3",
    "browser-sync-webpack-plugin": "2.3.0"
  },
  "scripts": {
    "replaceNames": "",
    "install-dev": "npm install && npm run dev",
    "install-prod": "npm install --only=production && npm run prod",
    "dev": "npm run development",
    "development": "mix",
    "watch": "mix watch",
    "watch-poll": "mix watch -- --watch-options-poll=1000",
    "hot": "mix watch --hot",
    "prod": "npm run production",
    "production": "mix --production"
  },
  "browserslist": [
    "> 1%",
    "last 2 versions"
  ]
}
